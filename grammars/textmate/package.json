{
  "name": "ain",
  "displayName": "ain",
  "description": "TextMate grammar for .ain files",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.101.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "ain",
      "aliases": ["ain", "ain"],
      "extensions": [".ain"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "ain",
      "scopeName": "source.ain",
      "path": "./syntaxes/ain.tmLanguage.json"
    }]
  }
}
